{
  "name": "honbot-api",
  "version": "0.0.0",
  "author": "Scott Cooper <scttcper@gmail.com>",
  "private": true,
  "scripts": {
    "typeorm": "typeorm",
    "makemigrations": "ts-node ./node_modules/.bin/typeorm migration:generate",
    "migrate": "ts-node ./node_modules/.bin/typeorm migration:run",
    "build": "del dist && tsc",
    "build:watch": "tsc -p . -w",
    "start": "NODE_ENV=prod DEBUG=honbot* nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/index.ts",
    "scanner": "NODE_ENV=dev DEBUG=honbot* nodemon --watch 'src/scanner.ts' --exec 'ts-node' src/scanner.ts",
    "test": "NODE_ENV=test mocha --exit --require ts-node/register test/*.spec.ts",
    "lint": "eslint -f codeframe src/**/**/*.ts test/**/**/*.ts",
    "lint:fix": "eslint -f codeframe --fix src/**/**/*.ts test/**/**/*.ts"
  },
  "dependencies": {
    "@hapi/hapi": "18.3.1",
    "@hapi/hoek": "8.0.2",
    "@hapi/joi": "15.1.0",
    "@sentry/node": "5.5.0",
    "boom": "7.3.0",
    "date-fns": "2.0.0-beta.2",
    "debug": "4.1.1",
    "good": "8.1.2",
    "good-console": "8.0.0",
    "good-squeeze": "5.1.0",
    "hapi-sentry": "2.0.2",
    "lodash": "4.17.14",
    "pg": "7.11.0",
    "redis": "2.8.0",
    "reflect-metadata": "0.1.13",
    "request": "2.88.0",
    "request-promise-native": "1.0.7",
    "semver": "6.2.0",
    "sqlite3": "4.0.9",
    "ts-trueskill": "2.5.0",
    "typeorm": "0.2.18"
  },
  "devDependencies": {
    "@types/boom": "7.2.1",
    "@types/debug": "4.1.4",
    "@types/hapi__hapi": "18.2.5",
    "@types/hapi__hoek": "6.2.0",
    "@types/hapi__joi": "15.0.2",
    "@types/joi": "14.3.3",
    "@types/lodash": "4.14.136",
    "@types/mocha": "5.2.7",
    "@types/node": "12.6.2",
    "@types/progress": "2.0.3",
    "@types/redis": "2.8.13",
    "@types/request-promise-native": "1.0.16",
    "@types/semver": "6.0.1",
    "@typescript-eslint/eslint-plugin": "1.11.0",
    "@typescript-eslint/parser": "1.11.0",
    "del-cli": "^2.0.0",
    "eslint": "6.0.1",
    "eslint-config-prettier": "6.0.0",
    "eslint-config-xo-space": "0.21.0",
    "eslint-config-xo-typescript": "0.15.0",
    "eslint-plugin-import": "2.18.0",
    "mocha": "6.1.4",
    "nodemon": "1.19.1",
    "progress": "2.0.3",
    "ts-node": "8.3.0",
    "typescript": "3.5.3"
  },
  "repository": "https://github.com/scttcper/honbot-api.git",
  "engines": {
    "node": ">=8.0.0"
  },
  "license": "MIT"
}
