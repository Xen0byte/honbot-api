{
  "name": "honbot-api",
  "version": "0.0.0",
  "author": "Scott Cooper <scttcper@gmail.com>",
  "private": true,
  "scripts": {
    "typeorm": "typeorm",
    "makemigrations": "ts-node ./node_modules/.bin/typeorm migration:generate",
    "migrate": "ts-node ./node_modules/.bin/typeorm migration:run",
    "build": "rimraf build; tsc -p .",
    "build:watch": "tsc -p . -w",
    "start": "NODE_ENV=prod DEBUG=honbot* nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/index.ts",
    "scanner": "NODE_ENV=dev DEBUG=honbot* nodemon --watch 'src/scanner.ts' --exec 'ts-node' src/scanner.ts",
    "test": "NODE_ENV=test mocha --exit --require ts-node/register test/*.spec.ts",
    "lint": "tslint \"src/**/*.ts\""
  },
  "dependencies": {
    "boom": "7.2.0",
    "catbox": "10.0.2",
    "catbox-redis": "4.1.0",
    "date-fns": "2.0.0-alpha.10",
    "debug": "3.1.0",
    "good": "8.1.1",
    "good-console": "7.1.0",
    "good-squeeze": "5.1.0",
    "hapi": "17.5.3",
    "hapi-raven": "7.0.0",
    "hoek": "5.0.4",
    "joi": "13.6.0",
    "lodash": "4.17.10",
    "pg": "7.4.3",
    "raven": "2.6.3",
    "redis": "2.8.0",
    "reflect-metadata": "0.1.12",
    "request": "2.88.0",
    "request-promise-native": "1.0.5",
    "semver": "5.5.0",
    "sequelize": "4.38.0",
    "sqlite3": "^4.0.2",
    "ts-trueskill": "2.2.3",
    "typeorm": "0.2.7"
  },
  "devDependencies": {
    "@types/boom": "7.2.0",
    "@types/catbox": "10.0.1",
    "@types/chai": "4.1.4",
    "@types/debug": "0.0.30",
    "@types/hapi": "17.0.17",
    "@types/hoek": "4.1.3",
    "@types/joi": "13.4.2",
    "@types/lodash": "4.14.116",
    "@types/mocha": "5.2.5",
    "@types/node": "10.5.8",
    "@types/progress": "2.0.1",
    "@types/raven": "2.5.1",
    "@types/redis": "2.8.6",
    "@types/request-promise-native": "1.0.15",
    "@types/semver": "5.5.0",
    "@types/sequelize": "4.27.24",
    "mocha": "5.2.0",
    "nodemon": "1.18.3",
    "progress": "2.0.0",
    "rimraf": "2.6.2",
    "ts-node": "7.0.0",
    "tslint": "5.11.0",
    "typescript": "3.0.1"
  },
  "repository": "https://github.com/scttcper/honbot-api.git",
  "engines": {
    "node": ">=7.6.0"
  },
  "license": "MIT"
}
