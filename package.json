{
  "name": "honbot-api",
  "version": "0.0.0",
  "author": "Scott Cooper <scttcper@gmail.com>",
  "private": true,
  "scripts": {
    "typeorm": "typeorm",
    "makemigrations": "ts-node ./node_modules/.bin/typeorm migration:generate",
    "migrate": "ts-node ./node_modules/.bin/typeorm migration:run",
    "build": "rimraf build; tsc -p .",
    "build:watch": "tsc -p . -w",
    "start": "NODE_ENV=prod DEBUG=honbot* nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/index.ts",
    "scanner": "NODE_ENV=dev DEBUG=honbot* nodemon --watch 'src/scanner.ts' --exec 'ts-node' src/scanner.ts",
    "test": "NODE_ENV=test mocha --exit --require ts-node/register test/*.spec.ts",
    "lint": "tslint \"src/**/*.ts\""
  },
  "dependencies": {
    "boom": "7.3.0",
    "date-fns": "2.0.0-alpha.26",
    "debug": "4.1.1",
    "good": "8.1.2",
    "good-console": "7.1.0",
    "good-squeeze": "5.1.0",
    "hapi": "18.0.1",
    "hapi-raven": "7.0.1",
    "hoek": "6.1.2",
    "joi": "14.3.1",
    "lodash": "4.17.11",
    "pg": "7.8.0",
    "raven": "2.6.4",
    "redis": "2.8.0",
    "reflect-metadata": "0.1.13",
    "request": "2.88.0",
    "request-promise-native": "1.0.5",
    "semver": "5.6.0",
    "sequelize": "4.42.0",
    "sqlite3": "4.0.6",
    "ts-trueskill": "2.3.0",
    "typeorm": "0.2.12"
  },
  "devDependencies": {
    "@types/boom": "7.2.1",
    "@types/chai": "4.1.7",
    "@types/debug": "0.0.31",
    "@types/hapi": "18.0.0",
    "@types/hoek": "4.1.3",
    "@types/joi": "14.3.1",
    "@types/lodash": "4.14.120",
    "@types/mocha": "5.2.5",
    "@types/node": "10.12.21",
    "@types/progress": "2.0.3",
    "@types/raven": "2.5.2",
    "@types/redis": "2.8.10",
    "@types/request-promise-native": "1.0.15",
    "@types/semver": "5.5.0",
    "@types/sequelize": "4.27.34",
    "mocha": "5.2.0",
    "nodemon": "1.18.9",
    "progress": "2.0.3",
    "rimraf": "2.6.3",
    "ts-node": "8.0.2",
    "tslint": "5.12.1",
    "typescript": "3.3.1"
  },
  "repository": "https://github.com/scttcper/honbot-api.git",
  "engines": {
    "node": ">=8.0.0"
  },
  "license": "MIT"
}
